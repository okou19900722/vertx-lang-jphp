        /*System.err.println(methodName + "___" + methods.size() + "____" + mmm.params.size());*/
        foreach(param : mmm.params) {
            var kind = param.type.kind;
            var x = getTypeScore(param.type);
            var list = map.get("_" + i);
            if(list == null) {
                list = new HashSet();
                map.put("_" + i, list);
            }
            list.add(x);
            i++;
            /*System.err.println(i + "-------------------" + list);*/
        }
    }
}

