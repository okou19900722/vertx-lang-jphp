		@foreach{str : split(doc.firstSentence.value)}
		 * @{str}\n
		@end{}
	@end{}
	@if{doc.body != null}
		@foreach{str : split(doc.body.value)}
		 * @{str}\n
		@end{}
	@end{}
@end{}
