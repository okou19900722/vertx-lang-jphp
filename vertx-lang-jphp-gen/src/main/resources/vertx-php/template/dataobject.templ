		if(type.simpleName == superType.simpleName) {
			importClassSet.add(superType.raw.translatePackageName("jphp").replace(".", "\\") + "\\" + superType.simpleName + " as J" + superType.simpleName);
			superClass = "J" + superType.simpleName;
		} else {
			addImport(superType);
			superClass = superType.simpleName;
		}
	}
	for(property : properties) {
		addImport(property.type);
	}
}

@foreach{importClass : importClassSet}
	use @{importClass.replace(".", "\\")};\n
@end{}
